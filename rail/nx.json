{
  "npmScope": "rail",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "prototype": {
      "tags": ["scope:app"]
    },
    "prototype-e2e": {
      "tags": [],
      "implicitDependencies": ["prototype"]
    },
    "corporate": {
      "tags": ["scope:shared"]
    },
    "admin": {
      "tags": ["scope:admin"]
    },
    "trains": {
      "tags": ["scope:trains"]
    }
  }
}
